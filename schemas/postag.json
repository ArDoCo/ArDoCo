          "tag": "IN",
          "tag": "CD",
          "tag": "DT",
          "tag": "WDT",
          "tag": "EX",
          "tag": "FW",
          "tag": "LS",
          "tag": "NN",
          "tag": "NNS",
          "tag": "NNP",
          "tag": "NNPS",
          "tag": "PDT",
          "tag": "POS",
          "tag": "PRP",
          "tag": "PRP$",
          "tag": "WP$",
          "tag": "WP",
          "tag": "RP",
          "tag": "SYM",
          "tag": "TO",
          "tag": "UH",
          "tag": "VB",
          "tag": "VBD",
          "tag": "VBG",
          "tag": "VBN",
          "tag": "VBP",
          "tag": "VBZ",
          "tag": "MD",
